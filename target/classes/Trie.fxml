<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TrieController">
    <Label layoutX="14.0" layoutY="14.0" text="Amount of users" />
    <Label layoutX="14.0" layoutY="61.0" text="Amount of attributes" />
    <TextField fx:id="tfAmountOfUsers" layoutX="217.0" layoutY="9.0" text="3" />
    <TextField fx:id="tfAmountOfAttributes" layoutX="217.0" layoutY="56.0" text="3" />
    <Button layoutX="14.0" layoutY="204.0" mnemonicParsing="false" onAction="#generateTrie" text="Generate Trie" />
    <Label layoutX="14.0" layoutY="101.0" text="Key" />
    <Label layoutX="14.0" layoutY="139.0" text="Value" />
    <TextField fx:id="tfKey" layoutX="217.0" layoutY="101.0" text="" />
    <TextField fx:id="tfValue" layoutX="217.0" layoutY="139.0" text="" />
    <Button layoutX="14.0" layoutY="242.0" mnemonicParsing="false" onAction="#getValue" prefWidth="109.0" text="Get value" />
    <Button layoutX="14.0" layoutY="280.0" mnemonicParsing="false" onAction="#insertValue" prefWidth="109.0" text="Insert value" />
    <Button layoutX="14.0" layoutY="318.0" mnemonicParsing="false" onAction="#dumpTrie" prefWidth="109.0" text="Dump trie" />
</AnchorPane>
